{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./public",
    "baseUrl": "./src",
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "typeRoots": ["./types"]
    
    
  },
 
  "include": ["./src/**/*.ts"],
  "exclude": ["./node_modules"]
}